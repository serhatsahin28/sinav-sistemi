<%-include("yonetimHeader")%>

    <div class="container">
      <div class="row justify-content-center">
        <%result.forEach((r)=>{
          %>

          <div class="col-md-5 ">
            <h1>Home</h1>
            <div class="card">
              <div class="card-body col-12">



                <h5 class="card-title">Öğrenci Bilgileri</h5>
                <p class="card-text">Öğrenci ismi: <span id="studentName">
                    <%=r.Ad%>
                  </span></p>
                <p class="card-text">Soyisim: <span id="studentSurname">
                    <%=r.Soyad%>
                  </span></p>
                <p class="card-text">Numara: <span id="studentAge">
                    <%=r.No%>
                  </span></p>





                <% });%>
              </div>
            </div>
          </div>
      </div>

    </div>







  </div>





  <%-include("yonetimFooter")%>
